{
    "name": "zopensap_travel_list_v4",
    "version": "0.0.1",
    "private": true,
    "sapux": true,
    "sapuxLayer": "CUSTOMER_BASE",
    "description": "A Fiori application.",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "start": "fiori run --open 'test/flpSandbox.html#comacwardzopensaptravellistv4-tile'",
        "start-local": "fiori run --config ./ui5-local.yaml --open 'test/flpSandbox.html#comacwardzopensaptravellistv4-tile'",
        "start-noflp": "fiori run --open 'index.html'",
        "start-variants-management": "fiori run --open \"preview.html?fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "deploy": "fiori cfDeploy",
        "deploy-config": "fiori add deploy-config",
        "int-tests": "fiori run --config ./ui5-mock.yaml --open 'test/integration/opaTests.qunit.html'",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open 'test/flpSandbox.html#comacwardzopensaptravellistv4-tile'",
        "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo",
        "build:mta": "rimraf resources mta_archives && mbt build",
        "undeploy": "cf undeploy com-ac-ward-zopensaptravellistv4 --delete-services --delete-service-keys"
    },
    "devDependencies": {
        "@ui5/cli": "^2.14.1",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "@sap/ux-ui5-tooling": "1",
        "rimraf": "3.0.2",
        "@sap/ux-specification": "latest",
        "@sap/ux-ui5-fe-mockserver-middleware": "1",
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "ui5-task-zipper": "^0.3.1",
        "mbt": "^1.0.15"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap/ux-ui5-fe-mockserver-middleware",
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper",
            "mbt"
        ]
    }
}
